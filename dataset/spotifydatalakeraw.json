{
	"name": "spotifydatalakeraw",
	"properties": {
		"linkedServiceName": {
			"referenceName": "spotifydatalake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"filename": {
				"type": "string",
				"defaultValue": "@variables('filename')"
			},
			"year": {
				"type": "string",
				"defaultValue": "@variables('currentDate')[0]"
			},
			"month": {
				"type": "string",
				"defaultValue": "@variables('currentDate')[1]"
			},
			"day": {
				"type": "string",
				"defaultValue": "@variables('currentDate')[2]"
			}
		},
		"annotations": [],
		"type": "Json",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().filename",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('spotify/', dataset().year, '/', dataset().month, '/', dataset().day)",
					"type": "Expression"
				},
				"fileSystem": "raw"
			}
		},
		"schema": {}
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}